<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="35dip" />
            <stroke android:width="3dip" android:color="#000000" />
            <gradient android:angle="-45" android:startColor="#4C4A4A" android:endColor="#4C4A4A"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="30dip" />
            <stroke android:width="1dip" android:color="#5e7974" />
            <solid android:color="#b87737"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="35dip" />
            <stroke android:width="3dip" android:color="#000000" />
            <gradient android:angle="-45" android:startColor="#343232" android:endColor="#221F1F" />
        </shape>
    </item>
</selector>